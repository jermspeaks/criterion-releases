{
  "manifest_version": 3,
  "name": "Criterion Releases to Markdown Table",
  "version": "1.0",
  "description": "Converts HTML tables to Markdown tables.",
  "permissions": ["activeTab", "scripting", "clipboardWrite"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
